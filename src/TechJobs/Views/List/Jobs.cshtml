<h1>@ViewBag.title</h1>

<h3>@ViewBag.jobs.Count Result(s)</h3>

    @foreach (Dictionary<string, string> job in ViewBag.jobs)
    {
        <table class="job-listing">

            @foreach (KeyValuePair<string, string> attribs in job)
            {

                <tr>
                    <td>@attribs.Key</td>
                    <td>@attribs.Value</td>
                </tr>
            }
        </table>
    }